{
  "name": "pngcompress",
  "version": "1.0.0",
  "description": "A cross-platform image compression tool supporting PNG, JPG and JPEG with lossless and lossy compression modes.",
  "main": "dist-main/main/main.js",
  "scripts": {
    "dev": "concurrently \"vite\" \"tsc -w -p tsconfig.main.json\" \"wait-on tcp:5173 && electron .\"",
    "build": "tsc -p tsconfig.main.json && vite build && electron-builder",
    "build:mac": "tsc -p tsconfig.main.json && vite build && electron-builder --mac",
    "build:win": "tsc -p tsconfig.main.json && vite build && electron-builder --win",
    "release": "tsc -p tsconfig.main.json && vite build && electron-builder --mac --win --publish always",
    "preview": "vite preview"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "clsx": "^2.1.1",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "framer-motion": "^12.24.7",
    "postcss": "^8.5.6",
    "sass": "^1.97.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "wait-on": "^9.0.3"
  },
  "dependencies": {
    "fs-extra": "^11.3.3",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^8.0.0",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-optipng": "^8.0.0",
    "imagemin-pngquant": "^10.0.0",
    "mime-types": "^3.0.2",
    "p-limit": "^7.2.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sharp": "^0.34.5"
  },
  "optionalDependencies": {
    "@img/sharp-darwin-arm64": "^0.34.5",
    "@img/sharp-darwin-x64": "^0.34.5",
    "@img/sharp-libvips-darwin-x64": "^1.2.4"
  }
}